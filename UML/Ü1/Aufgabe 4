class Konto{
  
  Kunde[] zeichnungsberechtigter;
  private String bezeichnung;
  
  public saldo(){
    return GeldBetrag;
  }
  
  public einzahlen(int GeldBetrag){
    this.GeldBetrag = GeldBetrag;
    return;
  }
  
}

abstract class Kunde{
  
  Konto[] konto;
  
}

class Privatkunde extends Kunde{
  
  private String vorname;
  private String nachname;
  
  Adresse postAdresse = new Adresse(String strasse, int hausnummer, String plz, String ort);
  
}

class Geschaeftskunde extends Kunde{
  
  private String firmenname;
  
  Adresse domizilAdresse = new Adresse(String strasse, int hausnummer, String plz, String ort);
  
}

class Adresse{
  
  public Adresse(String strasse, int hausnummer, String plz, String ort){
    
    this.strasse = strasse;
    this.hausnummer = hausnummer;
    this.plz = plz;
    this.ort = ort;
    
  }
  
  private String strasse;
  private int hausnummer;
  private String plz;
  private String ort;
  
}
